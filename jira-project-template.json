      "subtask": false,
      "iconUrl": "/images/icons/issuetypes/story.svg"
    },
    {
      "name": "Task",
      "description": "A task that needs to be done",
      "subtask": false,
      "iconUrl": "/images/icons/issuetypes/task.svg"
    },
    {
      "name": "Bug",
      "description": "A problem which impairs or prevents the functions of the product",
