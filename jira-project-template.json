    }
  ],
  "customFields": {
    "epicName": {
      "name": "Epic Name",
      "description": "Short name for the epic",
      "type": "com.pyxis.greenhopper.jira:gh-epic-label",
      "searcherKey": "com.pyxis.greenhopper.jira:gh-epic-label-searcher"
    },
    "epicLink": {
      "name": "Epic Link",
      "description": "Link to parent epic",
      "type": "com.pyxis.greenhopper.jira:gh-epic-link",
      "searcherKey": "com.pyxis.greenhopper.jira:gh-epic-link-searcher"
    },
    "storyPoints": {
      "name": "Story Points",
      "description": "Estimate in story points",
      "type": "com.atlassian.jira.plugin.system.customfieldtypes:float",
      "searcherKey": "com.atlassian.jira.plugin.system.customfieldtypes:exactnumber"
    },
    "acceptanceCriteria": {
      "name": "Acceptance Criteria",
      "description": "Conditions that must be met for the story to be accepted",
      "type": "com.atlassian.jira.plugin.system.customfieldtypes:textarea",
      "searcherKey": "com.atlassian.jira.plugin.system.customfieldtypes:textsearcher"
    },
    "businessValue": {
      "name": "Business Value",
      "description": "The business value this delivers",
      "type": "com.atlassian.jira.plugin.system.customfieldtypes:select",
      "options": ["Critical", "High", "Medium", "Low"],
      "searcherKey": "com.atlassian.jira.plugin.system.customfieldtypes:multiselectsearcher"
    }
  },
  "workflows": {
    "epic": {
      "name": "Epic Workflow",
      "statuses": ["To Do", "In Progress", "Done"],
      "transitions": [
        {"from": "To Do", "to": "In Progress", "name": "Start Epic"},
        {"from": "In Progress", "to": "Done", "name": "Complete Epic"},
        {"from": "In Progress", "to": "To Do", "name": "Stop Progress"}
      ]
    },
    "story": {
      "name": "Story Workflow",
      "statuses": ["To Do", "In Progress", "In Review", "Testing", "Done"],
      "transitions": [
        {"from": "To Do", "to": "In Progress", "name": "Start Work"},
        {"from": "In Progress", "to": "In Review", "name": "Submit for Review"},
        {"from": "In Review", "to": "Testing", "name": "Approve"},
        {"from": "In Review", "to": "In Progress", "name": "Request Changes"},
        {"from": "Testing", "to": "Done", "name": "Pass Testing"},
        {"from": "Testing", "to": "In Progress", "name": "Failed Testing"}
      ]
    },
    "task": {
      "name": "Task Workflow",
      "statuses": ["To Do", "In Progress", "Done"],
      "transitions": [
        {"from": "To Do", "to": "In Progress", "name": "Start Progress"},
        {"from": "In Progress", "to": "Done", "name": "Resolve"},
        {"from": "In Progress", "to": "To Do", "name": "Stop Progress"}
      ]
    }
  },
  "screens": {
    "epic": {
      "fields": [
        "summary",
        "description",
        "epicName",
        "businessValue",
        "acceptanceCriteria",
        "labels",
        "components",
        "fixVersions",
        "priority",
        "assignee",
        "reporter"
      ]
    },
    "story": {
      "fields": [
        "summary",
        "description",
        "epicLink",
        "storyPoints",
        "acceptanceCriteria",
        "labels",
        "components",
        "fixVersions",
        "priority",
        "assignee",
        "reporter"
      ]
    }
  },
  "boards": {
    "main": {
      "name": "Main Board",
      "type": "scrum",
      "filter": "project = PROJ",
      "columns": [
        {"name": "To Do", "statuses": ["To Do"]},
        {"name": "In Progress", "statuses": ["In Progress"], "max": 5},
        {"name": "In Review", "statuses": ["In Review"], "max": 3},
        {"name": "Testing", "statuses": ["Testing"]},
        {"name": "Done", "statuses": ["Done"]}
      ],
      "swimlanes": "epicLink",
      "quickFilters": [
        {"name": "My Issues", "jql": "assignee = currentUser()"},
        {"name": "Blocked", "jql": "labels = blocked"},
        {"name": "High Priority", "jql": "priority in (Critical, High)"}
      ]
    }
  },
  "components": [
    {"name": "Backend", "description": "Server-side logic and APIs", "lead": null},
