    {"name": "High", "color": "#FF9900", "description": "Important, do soon"},
    {"name": "Medium", "color": "#FFD700", "description": "Normal priority"},
    {"name": "Low", "color": "#0066CC", "description": "Nice to have"}
  ],
  "resolutions": [
    "Done",
    "Won't Do",
    "Duplicate",
